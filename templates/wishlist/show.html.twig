            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_wishlist_index') }}">back to list</a>

    <a href="{{ path('app_wishlist_edit', {'id': wishlist.id}) }}">edit</a>

    {{ include('wishlist/_delete_form.html.twig') }}
